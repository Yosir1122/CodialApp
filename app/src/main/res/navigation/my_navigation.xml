<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.codialapp.fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_kurslarFragment"
            app:destination="@id/kurslarFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mentorlarFragment"
            app:destination="@id/mentorlarFragment" />
        <action
            android:id="@+id/action_homeFragment_to_guruhlarFragment"
            app:destination="@id/guruhlarFragment" />
    </fragment>
    <fragment
        android:id="@+id/kurslarFragment"
        android:name="com.example.codialapp.fragment.kurslar.KurslarFragment"
        android:label="fragment_kurslar"
        tools:layout="@layout/fragment_kurslar" >
        <action
            android:id="@+id/action_kurslarFragment_to_dataFragment2"
            app:destination="@id/dataFragment" />
    </fragment>
    <fragment
        android:id="@+id/mentorlarFragment"
        android:name="com.example.codialapp.fragment.mentorlar.MentorlarFragment"
        android:label="fragment_mentorlar"
        tools:layout="@layout/fragment_mentorlar" >
        <action
            android:id="@+id/action_mentorlarFragment_to_mentorAddFragment"
            app:destination="@id/mentorAddFragment" />
    </fragment>
    <fragment
        android:id="@+id/guruhlarFragment"
        android:name="com.example.codialapp.fragment.guruhlar.GuruhlarFragment"
        android:label="fragment_guruhlar"
        tools:layout="@layout/fragment_guruhlar" >
        <action
            android:id="@+id/action_guruhlarFragment_to_addGroupFragment"
            app:destination="@id/addGroupFragment" />
    </fragment>
    <fragment
        android:id="@+id/dataFragment"
        android:name="com.example.codialapp.fragment.kurslar.DataFragment"
        android:label="DataFragment" />
    <fragment
        android:id="@+id/mentorAddFragment"
        android:name="com.example.codialapp.fragment.mentorlar.MentorAddFragment"
        android:label="fragment_mentor_add"
        tools:layout="@layout/fragment_mentor_add" />
    <fragment
        android:id="@+id/addGroupFragment"
        android:name="com.example.codialapp.fragment.guruhlar.AddGroupFragment"
        android:label="fragment_add_group"
        tools:layout="@layout/fragment_add_group" />
    <fragment
        android:id="@+id/guruhQoshishFragment"
        android:name="com.example.codialapp.fragment.guruhlar.GuruhQoshishFragment"
        android:label="fragment_guruh_qoshish"
        tools:layout="@layout/fragment_guruh_qoshish" />
    <fragment
        android:id="@+id/ochilayotganGuruhFragment"
        android:name="com.example.codialapp.fragment.guruhlar.OchilayotganGuruhFragment"
        android:label="fragment_ochilayotgan_guruh"
        tools:layout="@layout/fragment_ochilayotgan_guruh" />
    <fragment
        android:id="@+id/ochilganGuruhFragment"
        android:name="com.example.codialapp.fragment.guruhlar.OchilganGuruhFragment"
        android:label="fragment_ochilgan_guruh"
        tools:layout="@layout/fragment_ochilgan_guruh" />
    <fragment
        android:id="@+id/studentFragment"
        android:name="com.example.codialapp.fragment.guruhlar.StudentFragment"
        android:label="fragment_student"
        tools:layout="@layout/fragment_student" />
    <fragment
        android:id="@+id/editStudentFragment"
        android:name="com.example.codialapp.fragment.guruhlar.EditStudentFragment"
        android:label="fragment_edit_student"
        tools:layout="@layout/fragment_edit_student" />
    <fragment
        android:id="@+id/addStudentFragment"
        android:name="com.example.codialapp.fragment.guruhlar.AddStudentFragment"
        android:label="fragment_add_student"
        tools:layout="@layout/fragment_add_student" />
    <fragment
        android:id="@+id/editGroupFragment"
        android:name="com.example.codialapp.fragment.guruhlar.EditGroupFragment"
        android:label="fragment_edit_group"
        tools:layout="@layout/fragment_edit_group" />
</navigation>
